{
    "compilerOptions": {
      "strict": true,
      "module": "CommonJS",
      "moduleResolution": "Node",
      "target": "ESNext",
      "sourceMap": true,
      "outDir": "dist",
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@/*": [ "./src/*" ]
      }
    },
    "include": ["./src/**/*"],
    "exclude": [ "node_modules" ],
  }